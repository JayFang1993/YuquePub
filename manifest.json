{
  "name": "语雀",
  "version": "1.0",
  "description": "语雀批量发布",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_start",
    "all_frames": true
  }],
  "manifest_version": 2,
  "permissions": [
    "*://www.yuque.com/*"
  ],
  "web_accessible_resources": ["js/yuque.js", "js/jquery.js"]
}